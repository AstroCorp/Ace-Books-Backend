{
	"version": 2,
	"builds": [
		{
			"src": "src/infrastructure/app/main.ts",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/(.*)",
			"dest": "src/infrastructure/app/main.ts"
		}
	],
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Access-Control-Allow-Credentials",
					"value": "true"
				},
				{
					"key": "Access-Control-Allow-Origin",
					"value": "*"
				},
				{
					"key": "Access-Control-Allow-Methods",
					"value": "GET,OPTIONS,PATCH,DELETE,POST,PUT"
				}
			]
		}
	]
}

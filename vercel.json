{
	"$schema": "https://openapi.vercel.sh/vercel.json",
	"version": 2,
	"buildCommand": "npm run build",
	"outputDirectory": "dist",
	"functions": {
		"infrastructure/app/main.js": {
			"runtime": "@vercel/node"
		}
	},
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/infrastructure/app/main.js"
		}
	]
}
